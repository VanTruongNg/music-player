# App
APP_PORT=3001 # App port, e.g. 3001
GRPC_PORT=50052 # gRPC server port, e.g. 50052
APP_ENV=development # App environment: development | production

# PostgreSQL
POSTGRES_HOST=localhost # PostgreSQL host, e.g. localhost
POSTGRES_PORT=5432 # PostgreSQL port, e.g. 5432
POSTGRES_USER=postgres # PostgreSQL user, e.g. postgres
POSTGRES_PASSWORD=your_postgres_password # PostgreSQL password
POSTGRES_DB=music_player # PostgreSQL database name

# Redis
REDIS_HOST=localhost # Redis host, e.g. localhost
REDIS_PORT=6379 # Redis port, e.g. 6379
REDIS_PASSWORD=your_redis_password # Redis password
REDIS_USERNAME=your_redis_username # Redis username (if any)

# Kafka
KAFKA_BROKERS=localhost:9092 # Kafka broker list, comma separated
KAFKA_GROUP_ID=your_kafka_group # Kafka group id
KAFKA_CLIENT_ID=your_kafka_client # Kafka client id

# JWT
JWT_ACCESS_SECRET=your_jwt_access_secret # JWT access token secret
JWT_REFRESH_SECRET=your_jwt_refresh_secret # JWT refresh token secret
JWT_ACCESS_TTL=3600 # JWT access token time to live in seconds
JWT_REFRESH_TTL=604800 # JWT refresh token time to live in seconds
